#include <iostream>

union mima
{
  unsigned long birthday;
  unsigned short ssn;
  char* pet;
};

int main()
{
  mima mima_1;
  
  mima_1.birthday = 19991201;
  std::cout << mima_1.birthday << "\n";

  mima_1.pet = "mayas";

  std::cout << mima_1.pet <<"\n";
  std::cout << mima_1.birthday << "\n";

  return 0;
}
